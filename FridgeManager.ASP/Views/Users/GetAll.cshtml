@model List<UserDto>

@{
    ViewData["Title"] = "Users";
}

<div>
    <h3 class="h3-above-table">Users</h3>
    <table class="table">
        <thead>
            <tr>
                <th>Full Name</th>
                <th>UserName</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var fridgeModel in @Model)
            {
                <tr>
                    <td>@fridgeModel.FullName</td>
                    <td>@fridgeModel.UserName</td>
                    <td>@fridgeModel.Email</td>
                </tr>
            }
        </tbody>
    </table>
    <a class="back-btn" href="/Home/Index">Back</a>
</div>